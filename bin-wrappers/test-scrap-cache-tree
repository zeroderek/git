#!/bin/sh

# wrap-for-bin.sh: Template for git executable wrapper scripts
# to run test suite against sandbox, but with only bindir-installed
# executables in PATH.  The Makefile copies this into various
# files in bin-wrappers, substituting
# /c/BuildAgent/_work/11/s and t/helper/test-scrap-cache-tree.exe.

test -n "${GIT_EXEC_PATH##*:*}" ||
		GIT_EXEC_PATH="$(cygpath -u "$GIT_EXEC_PATH")"
if test -n "$NO_SET_GIT_TEMPLATE_DIR"
then
	unset GIT_TEMPLATE_DIR
else
	GIT_TEMPLATE_DIR="$GIT_EXEC_PATH"'/templates/blt'
	export GIT_TEMPLATE_DIR
fi
GITPERLLIB="$GIT_EXEC_PATH"'/perl/blib/lib'"${GITPERLLIB:+:$GITPERLLIB}"
GIT_TEXTDOMAINDIR="$GIT_EXEC_PATH"'/po/build/locale'
PATH="$GIT_EXEC_PATH"'/bin-wrappers:'"$PATH":"$GIT_EXEC_PATH"

export GIT_EXEC_PATH GITPERLLIB PATH GIT_TEXTDOMAINDIR

if test -n "$GIT_TEST_GDB"
then
	unset GIT_TEST_GDB
	exec gdb --args "${GIT_EXEC_PATH}/t/helper/test-scrap-cache-tree.exe" "$@"
else
	exec "${GIT_EXEC_PATH}/t/helper/test-scrap-cache-tree.exe" "$@"
fi
